{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server",
    "server:build": "tsc --project tsconfig.server.json",
    "client:build": "vite build",
    "storybook:build": "build-storybook -o dist/storybook",
    "build": "npm run server:build && npm run client:build",
    "extrabuild": "npm run server:build && npm run client:build && npm run storybook:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "storybook": "start-storybook -p 6006",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.4.0-beta.19",
    "@storybook/addon-essentials": "^6.4.0-beta.19",
    "@storybook/addon-links": "^6.4.0-beta.19",
    "@storybook/react": "^6.4.0-beta.19",
    "@types/express": "^4.17.13",
    "@types/nedb": "^1.8.12",
    "@types/node": "^16.11.4",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.4",
    "prettier": "2.4.1",
    "storybook-builder-vite": "^0.1.2",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.10"
  },
  "dependencies": {
    "@types/node-json-db": "^0.9.3",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "dotenv": "^10.0.0",
    "effector": "^22.3.0",
    "effector-react": "^22.3.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-subdomain": "^1.0.5",
    "lowdb": "^3.0.0",
    "mime": "^1.6.0",
    "nedb": "^1.8.0",
    "nft.storage": "^7.0.0",
    "node-json-db": "^2.1.3",
    "pg": "^8.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.35.0",
    "react-router-dom": "^5.3.0",
    "ts-node": "^10.9.1",
    "use-http": "^1.0.27",
    "web3": "^1.8.0"
  }
}
